{
    "backgroundColor": "#ffffff",
    "topBarBackgroundColor": "#f5f5f5",
    "font": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
    "customPatterns": [
        {
            "regex": "\\[.*?\\]",
            "style": "font-weight: bold;"
        },
        {
            "regex": "```(?:lua)?([\\s\\S]*?)```",
            "replace": "$1",
            "style": "font-family: monospace; tab-size: 4; background: #f4f4f4; display: block; border-radius: 4px; font-weight: normal;"
        }
    ]
}